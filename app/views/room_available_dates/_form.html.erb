<%= form_for @room_available_date do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :room_id %><br />
    <%= f.text_field :room_id %>
  </p>
  <p>
    <%= f.label :begin_at %><br />
    <%= f.date_select :begin_at %>
  </p>
  <p>
    <%= f.label :end_at %><br />
    <%= f.date_select :end_at %>
  </p>
  <p><%= f.submit %></p>
<% end %>
